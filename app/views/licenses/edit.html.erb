<%= form_with(model: @license, local: true, class: "form") do |form| %>
  <div class="form-group">
    <%= form.label :sent, class: "form-check-label" %>
    <%= form.check_box :sent, class: "form-check-input" %>
  </div>

  <div class="form-group">
    <%= form.label :revoked, class: "form-check-label" %>
    <%= form.check_box :revoked, class: "form-check-input" %>
  </div>

  <div class="form-group">
    <%= form.label :fiscal_year %>
    <%= form.text_field :fiscal_year, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :license_number %>
    <%= form.text_field :license_number, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :expiration_date %>
    <%= form.date_field :expiration_date, class: "form-control" %>
  </div>
  
  <div class="form-group">
    <%= form.label :date_issued %>
    <%= form.date_field :date_issued, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= form.label :conditions %>
    <%= form.text_field :conditions, class: "form-control" %>
  </div>
  
  <%= form.submit "Submit", class: "btn btn-primary mt-3" %>
<% end %>