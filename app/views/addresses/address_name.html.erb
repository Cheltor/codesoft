<div class="container mt-5"><div class="row"><div class="col">


<h1>Add Address Name for <%= @address.combadd %></h1>

<%= form_for @address do |f| %>
  <div class="form-group">
    <label for="property_name">Address Name</label>
    <%= f.text_field :property_name, class: "form-control" %>
  </div>

  <%= f.submit "Update Address", class: "btn btn-primary mt-3" %>
<% end %>
</div>
</div>
</div>
